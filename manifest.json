{
  "manifest_version": 2,
  "name": "Focus First Non-Pinned Tab",
  "version": "1.0",
  "description": "Maps Alt+0 to focus the first non-pinned tab",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "focus-first-non-pinned-tab": {
      "suggested_key": {
        "default": "Alt+0"
      },
      "description": "Focus the first non-pinned tab"
    }
  }
}
